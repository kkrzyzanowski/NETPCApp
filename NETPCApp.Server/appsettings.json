{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Jwt": {
    "Key": "TestSecretKey1234567890TestSecretKey12345678", // Replace this with a strong secret key
    "Issuer": "NETPCApp", // Replace with your app's name or domain
    "Audience": "NETPCApp", // Replace with the intended audience of the token
    "ExpiryMinutes": 60 // Token expiration time in minutes
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDb)\\MSSQLLocalDB;Database=NETPCAppDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
